{
	"name": "${template:DEVCONTAINER_NAME}",
	"context": "..",
	"workspaceFolder": "/workspace/",
	"dockerComposeFile": "docker-compose.yml",
	"service": "${template:DEVCONTAINER_NAME}-${template:DEVCONTAINER_ID}-devcontainer",
	"shutdownAction": "${template:DEVCONTAINER_SHUTDOWN_ACTION}",
    "settings": { "terminal.integrated.shell.linux": "${template:DEVCONTAINER_SHELL}" }
}